      "source": [
        "#消費税10%を設定\n",
        "shohizei = 0.1\n",
        "#税込み金額totalを定義\n",
        "total = 0\n",
        "#消費税を加算するための関数を定義\n",
        "def add_shohizei(price, shohizei):\n",
        "  total = price + price * shohizei\n",
        "  return total\n",
        "\n",
        "#関数add_shohizeiを実行し、戻り値を変数totalに代入\n",
        "total = add_shohizei(1500, shohizei)\n",
        "#計算結果を出力\n",
        "print(f\"1500円の商品の税込み価格は{total}円です。\")\n"
      ]
    }
