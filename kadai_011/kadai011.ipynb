      "source": [
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "#カウント用変数iの宣言\n",
        "i = 1\n",
        "#配列の値の出力を2回繰り返すためのwhile文\n",
        "while i < 3:\n",
        "\n",
        "#配列arrayの値を出力するためのfor文\n",
        "  for name in array:\n",
        "    print(name)\n",
        "  i = i + 1\n",
        "\n",
        "  if i >= 5:\n",
        "    print(\"5回目以降の出力処理となったため強制終了します。\")\n",
        "    break"
      ]
