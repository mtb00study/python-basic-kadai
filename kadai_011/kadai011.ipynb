      "source": [
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "#配列arrayの値を出力するためのfor文\n",
        "for name in array:\n",
        "  print(name)\n",
        "\n",
        "#配列arrayの値を出力するためのwhile文\n",
        "#インデックス用変数iを宣言\n",
        "i = 0\n",
        "while i <= 8:\n",
        "  name = array[i]\n",
        "  print(name)\n",
        "  i = i + 1\n",
        "\n",
        "\n",
        "\n"
